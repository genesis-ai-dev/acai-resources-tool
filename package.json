{
  "name": "acai-resources-tool",
  "displayName": "acai-resources-tool",
  "description": "provides acai resources for codex",
  "version": "0.0.1",
  "publisher": "project-accelarate",
  "type": "module",
  "engines": {
    "vscode": "^1.78.0"
  },
  "extensionKind": ["workspace"],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.mjs",
  "contributes": {
    "commands": [
      {
        "command": "acai-resources-tool.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "acai-resources-tool.openWebview",
        "title": "Open ACAI Webview"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "acai-resources",
          "title": "ACAI Resources",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "acai-resources": [
        {
          "id": "acaiResourcesView",
          "name": "Resources"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack && npm run build-webview",
    "watch": "concurrently \"webpack --watch\" \"npm run watch-webview\"",
    "package": "webpack --mode production --devtool hidden-source-map && rollup -c webviews/rollup.config.js --environment production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build-webview": "rollup -c webviews/rollup.config.js",
    "watch-webview": "rollup -c webviews/rollup.config.js -w"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tsconfig/svelte": "^5.0.4",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/rollup-plugin-css-only": "^3.1.3",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "concurrently": "^6.5.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-svelte": "^7.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.59.2",
    "svelte-loader": "^3.2.3",
    "svelte-preprocess": "^4.10.7",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.5",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  }
}
